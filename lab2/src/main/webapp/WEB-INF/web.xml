<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <!-- Servlet Declarations -->
  <servlet>
    <servlet-name>ClearServlet</servlet-name>
    <servlet-class>ClearServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClearServlet</servlet-name>
    <url-pattern>/clear</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>InitServlet</servlet-name>
    <servlet-class>InitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitServlet</servlet-name>
    <url-pattern>/init</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PointCheckServlet</servlet-name>
    <servlet-class>PointCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PointCheckServlet</servlet-name>
    <url-pattern>/pointCheck</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AreaCheckServlet</servlet-name>
    <servlet-class>AreaCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AreaCheckServlet</servlet-name>
    <url-pattern>/checkArea</url-pattern>
  </servlet-mapping>

  <!-- Filter Declaration -->
  <filter>
    <filter-name>LastModifiedFilter</filter-name>
    <filter-class>LastModifiedFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LastModifiedFilter</filter-name>
    <url-pattern>/init</url-pattern>
  </filter-mapping>

  <!-- Welcome File -->
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
